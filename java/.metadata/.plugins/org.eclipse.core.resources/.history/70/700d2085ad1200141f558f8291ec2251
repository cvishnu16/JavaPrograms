

public class myfirst {
public static void main(String args[])

{int j=0;
	System.out.println(Integer.SIZE);//int i;
	for( int i = 3; i < 5; i++)
	{
		do {
			
			System.out.print(" "+j);
			j++;
			}
		while(j!=i);
		
		System.out.print("\n");
	}
		
		
		
		
		
		
		
	//System.out.print(i);
	//System.out.println(-2>>1);
}
}